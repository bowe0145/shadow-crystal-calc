{
  "extends": ["tslint-config-standard", "tslint-react", "tslint-eslint-rules"],
  "rules": {
    "no-floating-promises": false,
    "no-unused-variable": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
